<h2>個人ページ</h2>

<li>
 <div class="text">自分の投稿した記事</div>
 <% @artcles.each do |article| %>
    <%= link_to article.title ,article_path(article.id) %>
    <%= image_tag article.image.url %>
    <i class="fa fa-heart"></i>
    <%= article.likes.count %>
 <% end %>
</li>

<li>
 <%= link_to likes_user_path(@user.id) do %>
  <div class="text">いいねした記事</div>
  <div class="num"><%= @user.likes.count %></div>
 <% end %>
</li>
    

